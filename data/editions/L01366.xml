                    <addrLine>neben <rs type="place" ref="#113">
                            <hi rend="latintype">Türkenschanz</hi>park</rs>
                    </addrLine>
                    <addrLine>
                        <hi rend="latintype">
                            <rs ref="#41240" type="place">Austria</rs>

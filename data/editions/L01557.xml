        <body>
            <div type="address">
                <address>
                    <addrLine>
                        <pb facs="MS-SCHNITZLER-B-00043-B-00002-000-00221"/>
                        <hi rend="latintype">Herrn D<hi rend="superscript">r</hi> Arthur Schnitzler</hi>
                    </addrLine>
                    <addrLine>
                        <rs type="place" ref="#50">
                            <hi rend="latintype">Wien</hi>
                        </rs>
                    </addrLine>
                    <addrLine>
                        <rs ref="#133" type="place">
                            <hi rend="latintype">XVIII Spöttelgasse 7</hi>.</rs>
                    </addrLine>

            <div type="writingSession" n="1">
                <opener>
                    <p rend="center">
                        <pb facs="MS-SCHNITZLER-B-00043-B-00001-000-00271"/>
                        <rs ref="#pmb34026" type="place">
                            <hi rend="pre-print">
                                <hi rend="bold">K. B. Telegraphen<c rendition="#langesS">s</c>tation</hi>
                            </hi>
                            <space unit="chars" quantity="1"/>
                            <add place="inline">
                                <anchor type="textConst" xml:id="TL2222-1"/>TEGERNSEE<note type="textConst" xml:id="TL2222-1h">Stempel.</note>
                            </add>
                        </rs>
                    </p>
                    <p rend="center">
                        <hi rend="pre-print">Telegramm.</hi>
                    </p>
                    <p>
                        <hi rend="pre-print">Nr.</hi>
                        <space unit="chars" quantity="1"/>310</p>
                    <p>
                        <seg rend="left">
                            <hi rend="pre-print">Aufgegeben in</hi>
                            <space unit="chars" quantity="1"/>
                            <rs ref="#pmb75" type="place">Hinterbrühl</rs>
                        </seg>
                        <seg rend="right">
                            <hi rend="pre-print">Abgefertigt</hi>
                            <space unit="chars" quantity="1"/>
                            <date>7/8<space unit="chars" quantity="1"/>
                                <hi rend="pre-print">189</hi>8</date>
                            <space unit="chars" quantity="1"/>
                            <time>11<hi rend="pre-print">U</hi>10<hi rend="pre-print">M</hi>
                                <space unit="chars" quantity="1"/>V<hi rend="pre-print">Mttg.</hi>
                            </time>
                        </seg>
                    </p>
                    <p>
                        <hi rend="pre-print">Nr.</hi>
                        <space unit="chars" quantity="1"/>103 0 30<hi rend="pre-print">W.</hi>
                    </p>
                    <p>
                        <hi rend="pre-print">den</hi>
                        <space unit="chars" quantity="1"/>
                        <date>7/8 <hi rend="pre-print">189</hi>8</date>
                        <space unit="chars" quantity="1"/>
                        <time>9<hi rend="pre-print">U</hi>–<hi rend="pre-print">M</hi>
                            <space unit="chars" quantity="1"/>V<hi rend="pre-print">Mttg.</hi>
                        </time>
                    </p>
                </opener>
                <p rend="center">Schnitzler<space unit="chars" quantity="5"/>
                    <rs ref="#pmb34048" type="place">Hotel Post</rs> über <rs ref="#pmb38129" type="place">München</rs>
                </p>
                <p>Bin aus vielen Gründen<space unit="chars" quantity="1"/>
                    <c rendition="#langesS">s</c>chon <date>Mittwoch</date>
                    <space unit="chars" quantity="1"/>
                    <time>Abend</time> in <rs ref="#pmb854" type="place">Ba<c rendition="#langesS">s</c>el</rs> bitte Drahtantwort <rs ref="#pmb75" type="place">Hinterbrühl</rs> ob<space unit="chars" quantity="1"/>
                    <c rendition="#langesS">s</c>ie<space unit="chars" quantity="1"/>
                    <c rendition="#langesS">s</c>päte<c rendition="#langesS">s</c>tens <date>Donner<c rendition="#langesS">s</c>tag</date> auch dort<space unit="chars" quantity="1"/>
                    <c rendition="#langesS">s</c>ein können und welches Ga<c rendition="#langesS">s</c>thaus</p>
                <closer>
                    <signed>Hugo</signed>
                </closer>
            </div>

        <acdh:hasLicensor rdf:resource="http://d-nb.info/gnd/1226158307"/> <!-- ACDH -->
        <acdh:hasContact rdf:resource="http://d-nb.info/gnd/1226158307"/> <!-- ACDH -->
        <acdh:hasOwner rdf:resource="http://d-nb.info/gnd/1226158307"/>
